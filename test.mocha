
SuppressWarnings(MEMORY)

class barray
{
    const int length
    const MemObject pointer

    operator subscript(int i)
    {
        return pointer[i]
    }
}

class String
{
    const int length
    const barray string;
}

class Passport
{
    String name
    String lastname
    String dateofbirth
    String nationality
    String countryofbirth

    Passport()
    {
    }

    void GetName()
    {
        return name + nationality * i
    }

    String GetLastName()
    {
        return lastname;
    }

    String GetDateOfBirth()
    {
        return dateofbirth
    }

    String GetNationality()
    {
        return nationality
    }

    String GetCountryOfBirth()
    {
        return countryofbirth.length
    }
}